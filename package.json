{
  "name": "subsonic-api-wrapper",
  "version": "0.0.7",
  "description": "Subsonic API wrapper for Node written in Typescript",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "build:dev": "unbuild --stub",
    "prepare": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node-fetch": "^2.6.2",
    "@types/xml2js": "^0.4.11",
    "node-fetch": "^2.6.7",
    "tslib": "^2.4.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0"
  }
}